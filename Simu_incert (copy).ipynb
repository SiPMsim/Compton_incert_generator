    "\n",
    "#Choose at least appropriate A, B, E, and nb_per_angle.\n",
    "C=1.055\n",
    "fwhm=0.8 # percentage uncertainty\n",
    "fwhm_E=0.140 # energy at which FWHM is measured kEV\n",
    "nb_per_angle=50000 #number of generated Monte Carlo per angle\n",
    "\n",
    "xpE = np.arange(0,E,0.000005)#Discrete value of possible Energies for E2\n",
    "val_alpha=np.arange(0,180,1) #range of compton angle\n",
    "\n",
    "\n",
    "sigmas_1=[]\n",
    "tot_E1=[]\n",
    "tot_E2=[]\n",
    "for i,theta in enumerate(val_alpha):\n",
    "    print(\"Calculating FWHM uncertainty for Compton angle = \", theta, \"°\")\n",
    "    pE2 = probs_E2(E,theta, xpE, xp)\n",
    "    tot_theta=[]\n",
    "    sumE2=[]\n",
    "    sumE1=[]\n",
    "    for i in range (nb_per_angle):\n",
    "        e2=tirer(xpE, pE2)\n",
    "        while e2==0:\n",
    "            e2=tirer(xpE, pE2)\n",
    "        e1=E-e2\n",
    "        sigE1 = fwhm/100 * sqrt(fwhm_E) / sqrt(e1) * e1 if e1 != 0 else 0\n",
    "        sigE2 = fwhm/100 * sqrt(fwhm_E) / sqrt(e2) * e2\n",
    "\n",
    "        sigE1=sigE1/2.355\n",
    "        sigE2=sigE2/2.355\n",
    "            \n",
    "        \n",
    "        val1=np.random.normal(0, sigE1)\n",
    "        val2=np.random.normal(0, sigE2)\n",
    "\n",
    "        while  e1 + val1 <=0 or e2 + val2<=0:\n",
    "            val1 = np.random.normal(0, sigE1)\n",
    "            val2 = np.random.normal(0, sigE2)\n",
    "        e1 = e1 + val1\n",
    "        e2 = e2 + val2\n",
    "        if not(np.isnan(np.degrees(theta_val(e1,e2)))):\n",
    "            tot_theta.append(np.degrees(theta_val(e1,e2)))\n",
    "            sumE1.append(e1)\n",
    "            sumE2.append(e2)\n",
    "    \n",
    "    tot_E1.append(np.mean(sumE1))\n",
    "    tot_E2.append(np.mean(sumE2))\n",
    "    fig1 = plt.figure()    \n",
    "    y,x,z=plt.hist(tot_theta, bins='auto', range=(0, 180));\n",
    "    x=x[:-1]\n",
    "    y=y/max(y)\n",
    "\n",
    "    sigmas_1.append(calculer_fwhm(x, y))\n",
    "    plt.close(fig1)\n"
