    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from math import *\n",
    "from scipy.optimize import curve_fit\n",
    "from scipy.integrate import quad \n",
    "from random import randrange\n",
    "from scipy import misc\n",
    "import random\n",
    "from scipy.signal import find_peaks, peak_widths\n",
    "from scipy.optimize import curve_fit\n",
    "\n",
    "def lorentz2(x,L,A,L2,A2):\n",
    "    return  A * L/(2*np.pi) * 1/(L**2/4 +x**2)+ A2 * L2/(2*np.pi) * 1/(L2**2/4 +x**2)\n",
    "\n",
    "def energy_E2(E0,theta):\n",
    "    return E0/(1+ E0/0.511 * (1- cos(np.radians(theta))))\n",
    "\n",
    "def theta_val(e1,e2):\n",
    "    return np.arccos((1-(-0.511 / (e1 + e2) + (0.511 / e2))))\n",
    "\n",
    "def calculer_fwhm(x, y):\n",
    "    demi_max = np.max(y) / 2\n",
    "\n",
    "    # Cherche les indices où la courbe croise la demi-hauteur\n",
    "    indices = np.where(y >= demi_max)[0]\n",
    "    if len(indices) < 2:\n",
    "        raise ValueError(\"Impossible de déterminer la FWHM : pic insuffisant ou données inadéquates.\")\n",
    "\n",
    "    gauche = indices[0]\n",
    "    droite = indices[-1]\n",
    "\n",
    "    # Interpolation linéaire à gauche\n",
    "    if gauche == 0:\n",
    "        x_gauche = x[gauche]\n",
    "    else:\n",
    "        x_gauche = x[gauche - 1] + (demi_max - y[gauche - 1]) * (x[gauche] - x[gauche - 1]) / (y[gauche] - y[gauche - 1])\n",
    "\n",
    "    # Interpolation linéaire à droite\n",
    "    if droite == len(x) - 1:\n",
    "        x_droite = x[droite]\n",
    "    else:\n",
    "        x_droite = x[droite] + (demi_max - y[droite]) * (x[droite + 1] - x[droite]) / (y[droite + 1] - y[droite])\n",
    "\n",
    "    fwhm = x_droite - x_gauche\n",
    "    return fwhm\n",
    "\n",
    "def find_pz(E0,E2, alpha_rad):\n",
    "    cos_alpha = np.cos(np.radians(alpha_rad))\n",
    "    numerator = E0 - E2 - (E0 * E2 * (1 - cos_alpha)) / (0.511)\n",
    "    denominator = np.sqrt(E0**2 + E2**2 - 2 * E0 * E2 * cos_alpha)\n",
    "    pz =  numerator / denominator\n",
    "    return -0.511*pz \n",
    "\n",
    "def proba_E(E,E2,Ec, theta,prob_p):\n",
    "    return prob_p*((Ec)/E+ E/(Ec) - (np.sin(np.radians(theta)))**2) * 1/np.sqrt(E**2+E2**2 - 2*E*E2*np.cos(np.radians(theta)))\n",
    "    \n",
    "def pdf(x,L,A,L2,A2): \n",
    "    pdf_x, err = quad(lorentz2, 0.0, x, args=(L,A,L2,A2,)) \n",
    "    return pdf_x\n",
    "\n",
    "\n",
    "def probs_E2(E,theta, xpE,xp):\n",
    "    Ec=energy_E2(E,theta)\n",
    "    pE2=[]\n",
    "    for E2 in xpE:\n",
    "        pz=find_pz(E,E2, theta)\n",
    "        prob_p=lorentz2(pz/(0.511*1/137),L,A,L2,A2) #mec**2/c*typical longitudinal momentum 1s hydrogen (1/137)\n",
    "        pE2.append(proba_E(E,E2,Ec,theta, prob_p))\n",
    "    pE2/max(pE2)    \n",
    "    \n",
    "    pE2 =np.cumsum(pE2)\n",
    "    return   pE2/max(pE2)\n",
    "    \n",
    "def tirer(E2,pE2):\n",
    "    ale=np.random.random()\n",
    "    pz_sortant=np.interp(ale,pE2,E2,)\n",
    "    #plt.plot(Q,pz_cumule)\n",
    "    return pz_sortant\n",
    "\n"
   ]
